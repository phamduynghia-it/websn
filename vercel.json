{
    "buildCommand": "npm run build",
    "outputDirectory": ".",
    "installCommand": "npm install",
    "framework": null,
    "routes": [
        {
            "src": "/src/(.*)",
            "dest": "/src/$1"
        },
        {
            "src": "/dist/(.*)",
            "dest": "/dist/$1"
        },
        {
            "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|mp3|ttf|woff|woff2))",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}
